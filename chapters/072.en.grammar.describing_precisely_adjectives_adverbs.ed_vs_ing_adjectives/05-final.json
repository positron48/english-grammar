  ],
  "learning_objectives": [
    "Отличать -ed прилагательные (чувство) от -ing (причина)",
    "Использовать -ed для описания людей, -ing для вещей/ситуаций",
    "Избегать типичных ошибок с bored/boring, interested/interesting",
    "Строить корректные предложения с common pairs"
  ],
  "estimated_minutes": 22,
  "blocks": [
    {
      "id": "b1_meaning_core",
      "type": "theory",
      "title": "-ed прилагательные описывают чувства человека: *bored, excited, interested*.",
      "theory": {
        "concept_id": "ed_ing_core_meaning",
        "content_md": "-**ed** прилагательные описывают **чувства** человека: *bored, excited, interested*.\n\n-**ing** прилагательные описывают **причину/впечатление**: *boring, exciting, interesting*.",
        "key_points": [],
        "common_mistakes": [],
        "examples": [
          {
            "id": "ex1",
            "text": "I am bored at home.",
            "translation": "Мне скучно дома.",
            "tags": [
              "affirmative"
            ]
          },
          {
            "id": "ex2",
            "text": "This book is boring.",
            "translation": "Эта книга скучная.",
            "tags": [
              "affirmative"
            ]
          }
        ]
      }
    },
    {
      "id": "b2_quiz_after_block",
      "type": "quiz_inline",
      "title": "Проверка знаний",
      "quiz_inline": {
